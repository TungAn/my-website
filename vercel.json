{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/glints-case-study",
      "dest": "/glints-case-study.html"
    },
    {
      "src": "/lazada-case-study",
      "dest": "/lazada-case-study.html"
    },
    {
      "src": "/popjam-case-study",
      "dest": "/popjam-case-study.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "silent": true
  }
} 